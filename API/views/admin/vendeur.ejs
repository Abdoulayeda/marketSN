<%- include('../master/header') -%> 

<div class="container">
    <h3>Contact du vendeur</h3>
    <h2> Email: <%=user.email%></h2>
    <h2> Téléphone: <%=user.telephone  %> </h2>
    <h1>Liste des produits</h1>
    <table class="table table-striped">
        <thead>
            <th>Nom</th>
            <th>Prix</th>
            <th>Quantite</th>
            <th>Image</th>
            <th>Actif</th>
            <th>Action</th>
        </thead>
        <tbody>
            <% for(produit of vendeur.produits) {%>
            <tr>
                <td><%=produit.nom%> </td>
                <td><%=produit.prix  %>FCFA </td>
                <td><%=produit.quantite  %></td>
                <td><img src="../images/<%=produit.image  %>" width="80px" height="80px"alt=""></td>
                <td><%=produit.actif%></td>
                <td><a href="" class="btn btn-danger">Supprimer</a></td>
            </tr>
            <% } %>
        </tbody>

    </table>
</div>
 




<%- include('../master/footer') -%> 